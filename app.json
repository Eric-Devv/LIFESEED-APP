{
  "expo": {
    "name": "Lifeseed",
    "slug": "lifeseed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
     /* "expo-sqlite",*/
     /*"expo-constants",*/
      "expo-notifications",
      "expo-secure-store",
      "expo-splash-screen",
     /* "expo-task-manager",*/
     /* "expo-background-fetch"*/
    ]
  }
}

